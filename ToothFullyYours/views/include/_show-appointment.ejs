<% for(var i =0; i < appoint.length; i++){%>
    <tr class="clickable-row">
        <td><%= i + 1 %></td>
        <td><%=appoint[i].p_name %></td>
        <td><%=appoint[i].p_email %></td>
        <td><%=appoint[i].p_ap_date %></td>
        <td><%=appoint[i].p_ap_time %></td>
        <td><%=appoint[i].p_procedure%></td>
        <td><%=appoint[i].p_phoneNum %></td>
        <td>
            <a class="d-flex jc-center delete" data-id=<%=appoint[i]._id%>>
                <span class="text-gradient"><i class="fa-solid fa-check" aria-hidden="1"></i></span>
            </a>
        </td>
    </tr>
    <% } %>